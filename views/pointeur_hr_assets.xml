<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="pointeur_hr_assets_backend" name="pointeur_hr assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/pointeur_hr/static/src/js/attendance_report_tree.js"/>
            </xpath>
        </template>

        <template id="pointeur_hr_attendance_report_buttons" name="pointeur_hr attendance report buttons">
            <t t-extend="ListView.buttons">
                <t t-jquery=".o_list_buttons" t-operation="append">
                    <button type="button" class="btn btn-primary o_list_button_export">
                        <i class="fa fa-download"/> Exporter
                    </button>
                </t>
            </t>
        </template>
    </data>
</odoo>
